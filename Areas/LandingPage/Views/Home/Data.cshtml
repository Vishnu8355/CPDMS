@model CPDMSEF.DataEF
@{
    <link rel="icon" type="image/x-icon" href="./images/departmentlogo.png">
    ViewData["Title"] = "Data";
    Layout = "~/Areas/LandingPage/Views/Shared/_LandingPageLayout.cshtml";
}
<link href="~/LandinCss/css/bootstrap.min.css" rel="stylesheet">
<!-- template-style-->
<link href="~/LandinCss/css/style1.css" rel="stylesheet">

<style>
    input[type=number]::-webkit-inner-spin-button,
    input[type=number]::-webkit-outer-spin-button {
        -webkit-appearance: none;
        -moz-appearance: none;
        appearance: none;
        margin: 0;
    }</style>

<div class="bt-content-wrap btClear section_Top">
    <div class="bt-content-holder">
        <div class="bt-content">
            <div class="bt_bb_wrapper">
                <section class="contact-page pad-tb bg-gradient5">
                   
                    <div class="container">
                        <div class="row justify-content-center">
                            <div class="col-lg-7">
                                <div class="niwax23form shadow">
                                    <div class="common-heading text-l">
                                        
                                        <h3 class="  text-[#024828] text-center">Data</h3>
                                        <p class="mb50 mt10">Fill all Mandatory Fields </p>
                                    </div>
                                    <div class="contact-form-card-pr contact-block-sw m0 iconin">
                                        <div class="form-block niwaxform">

                                            @using (Html.BeginForm("Data", "Home", FormMethod.Post, new { AUTOCOMPLETE = "Off", id = "form1" }))
                                                  {
                                            
                                                <div class="fieldsets row">
                                                    <div class="col-md-6 form-group floating-label">
                                                        <div class="formicon">
                                                            <i class="fas fa-file-alt"></i>
                                                        </div>
                                                        <select required="required" id="interested_in"
                                                                class="floating-select" name="Grpoup">
                                                            <option value="">Select Group</option>
                                                            <option value="Chemicals">Chemicals</option>
                                                            <option value="Petro-Chemicals">Petro-Chemicals</option>>
                                                        </select>
                                                       
                                                        <div class="error-label"></div>
                                                    </div>
                                                    <div class="col-md-6 form-group floating-label">
                                                        <div class="formicon">
                                                            <i class="fas fa-file-alt"></i>
                                                        </div>
                                                        <input type="text" name="CAS" required="required" placeholder="CAS NO"></input>
                                                        <div class="error-label"></div>
                                                    </div>
                                                </div>


                                                <div class="fieldsets row">
                                                   <div class="col-md-6 form-group floating-label">
                                                        <div class="formicon">
                                                            <i class="fas fa-file-alt"></i>
                                                        </div>
                                                        @*<input type="tel" name="HS" required="required" pattern="[0-9]{10}"></input>*@
                                                        @Html.TextBoxFor(m => m.HS, new { @class = "form-control", required = "required", placeholder="HS Code" , type="number" ,pattern="[0-9]{4}", maxlength="10", oninput="javascript: if (this.value.length > this.maxLength) this.value = this.value.slice(0, this.maxLength);"})
                                                        

                                                        <div class="error-label"></div>
                                                    </div>


                                                    <div class="col-md-6 form-group floating-label">
                                                        <div class="formicon">
                                                            <i class="fas fa-file-alt"></i>
                                                        </div>
                                                        @Html.DropDownListFor(m => m.Year, ViewBag.ViewFinYEarList as IEnumerable<SelectListItem>, "Select Financial Year", new { @class = "floating-select", autofocus = "autofocus",required="required"})
                                                        
                                                       
                                                        <div class="error-label"></div>
                                                    </div>
                                                   

                                                </div>

                                                <div class="fieldsets row">

                                                     <div class="col-md-12 form-group floating-label">
                                                        <div class="formicon">
                                                            <i class="fas fa-file-alt"></i>
                                                        </div>
                                                        @Html.DropDownListFor(m => m.Month, ViewBag.ViewMonthList as IEnumerable<SelectListItem>, "Select Month", new { @class = "floating-select", autofocus = "autofocus",required="required"})
                                                        @*<select required="required" id="interested_in"
                                                                class="floating-select" name="interested_in">
                                                            <option value="">Select Months</option>
                                                            <option value="Graphic Design">
                                                                January
                                                            </option>
                                                            <option value="Web Design">February</option>
                                                            <option value="App Design">March</option>
                                                            <option value="Other">April</option>
                                                        </select>*@
                                                    
                                                        <div class="error-label"></div>
                                                    </div>
                                                   
                                                </div>
                                        <div class="custom-control custom-checkbox ctmsetsw" >
                                            <input type="checkbox" class="custom-control-input ctminpt"
                                                   id="agree" name="agree" checked="checked" style="background-color: #024828 !important;">
                                            <label class="custom-control-label ctmlabl" for="agree">
                                                By clicking
                                                the “Submit” button you agree to our <a href="javascript:void(0)">
                                                    Terms &amp;
                                                    Conditions
                                                </a>.
                                            </label>
                                        </div>
                                                <div class="fieldsets mt20" style="width:20%!important">
                                                    <button type="submit" name="submit"
                                                        class="btn btn-main  w-fit mb20" style=" background-color: #024828 !important; color: #fff;">
                                                        Submit
                                                    </button>
                                                </div>
                                        }
                                      
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
            </div>

            <button type="button" class="btn btn-dark" style="margin-Left:300px" onclick="history.back()">
                <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-arrow-narrow-left" width="24" height="24" viewBox="0 0 24 24" stroke-width="1.5" stroke="#ffffff" fill="none" stroke-linecap="round" stroke-linejoin="round">
                    <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                    <path d="M5 12l14 0" />
                    <path d="M5 12l4 4" />
                    <path d="M5 12l4 -4" />
                </svg>
            </button>
        
            </section>
        </div>
    </div>

</div>