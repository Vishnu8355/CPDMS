        $('.SolventProcuredfrom').val(SolventProcuredfrom);
        $('.AnyOtherName').val(AnyOtherName);
        $('.AnyOtherQuantity').val(AnyOtherQuantity);
        $('.AnyOtherUnit').val(AnyOtherUnit);
        $('.AnyOtherPhysicalState').val(AnyOtherPhysicalState);
        $('.AnyOtherProcuredfrom').val(AnyOtherProcuredfrom);

        // remove the row from the table
        currentRow.remove();
        UpdateSerialNumbers();
    }

    function DeleteRow(row) {
        var currentRow = $(row).closest('tr');
        currentRow.remove();
        UpdateSerialNumbers();
    }
    function UpdateSerialNumbers() {
        $('#tblMain_Constituents_Data tbody tr').each(function (index) {
            $(this).find('td:first').text(index + 1);
        });
    }
</script>

