                                                    <img src="~/image/infoicon.png" title="Please Upload GST Certificate (Only .jpeg, .jpg, .png, .pdf file types are allowed and Max Size of Document is 700KB)" onclick="" />

                                                </div>
                                            </div>
                                            <div class="col-sm">
                                                <div class="form-group">
                                                    Pan Card
                                                    @Html.TextBoxFor(m => m.Pan_Certificate, new { @class = "form-control",type="file",required = "required",accept=".jpeg, .jpg, .png, .pdf",placeholder="" })
                                                    <img src="~/image/infoicon.png" title="Please Upload Pan Card (Only .jpeg, .jpg, .png, .pdf file types are allowed and Max Size of Document is 700KB)" onclick="" />

