                                </ul>
                                <div role="tabpanel">
                                    @using (Html.BeginForm("CompanyEntryAdd", "CompanyRegistration", FormMethod.Post, new { enctype = "multipart/form-data", AUTOCOMPLETE = "Off", id = "form1" }))
                                    {
                                        <div class="form-group bg-gray-300 p-2"> <button type="button" class=" text-left "> <b> Basic information</b> </button> </div>
                                         <div class="row">
                                            <div class="col-sm">
                                        <div class="form-group">

                                            Company Name <span class="RequiredField">*</span>
