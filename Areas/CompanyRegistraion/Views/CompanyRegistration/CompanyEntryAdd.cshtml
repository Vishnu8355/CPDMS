                                        <div class="form-group">
                                            <label style="color:red">
                                                <input type="checkbox" id="autoFillCheckbox"> Same as Registered Address

                                            </label>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm">
                                                <div class="form-group">
                                                    Communication Address <span class="RequiredField">*</span>
                                                    @Html.TextBoxFor(m => m.COffice_Address, new { @class = "form-control",id = "communicationAddress",required = "required",placeholder="Communication Address" })
@*                                                    <img src="~/image/infoicon.png" title="Please Enter Communication Address" onclick="" />
*@
                                                </div>
                                            </div>
                                        </div>

                                        <div class="row">
                                            <div class="col-sm">
                                                <div class="form-group">
                                                    State <span class="RequiredField">*</span>
                                                    @Html.DropDownListFor(m => m.State_Code, ViewBag.ViewStateList as IEnumerable<SelectListItem>, "-- Select State --", new { @class = "form-control", required = "required" })
@*                                                    <img src="~/image/infoicon.png" title="Please Select State" onclick="" />
*@
                                                    </div>
                                            </div>

                                            <div class="col-sm">
                                                <div class="form-group">
                                                    District <span class="RequiredField">*</span>
                                                    <select name="District_Code" id="District_Code" class="form-control searchableselect" required="required">
                                                        <option value="">-- Select District --</option>
                                                        @{
                                                            List<CPDMSEF.MasterEF> lstDistrictModel = (List<CPDMSEF.MasterEF>)ViewData["DistrictDetails"];
                                                            if (lstDistrictModel != null)
