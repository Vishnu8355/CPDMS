@*                                                    <img src="~/image/infoicon.png" title="Please Select Block Mention on Company Registration Certificate" onclick="" />
*@                                                </div>
                                            </div>
                                            <div class="col-sm">
                                                <div class="form-group">
                                                    Pin Code <span class="RequiredField">*</span>
@*                                                    <img src="~/image/infoicon.png" title="Please Enter Pin Code Mention on Company Registration Certificate" onclick="" />
*@    @Html.TextBoxFor(m => m.RPincode, new { @class = "form-control", required = "required", type = "number", pattern = "[0-9]{4}", maxlength = "6", oninput = "javascript: if (this.value.length > this.maxLength) this.value = this.value.slice(0, this.maxLength);", placeholder = "Enter PinCode" })
                                               
                                                </div>
