                                                    <img src="~/image/infoicon.png" title="Please Upload Registration Certificate (Only .jpeg, .jpg, .png, .pdf file types are allowed and Max Size of Document is 700KB)" onclick="" />

                                                </div>
                                            </div>
                                            <div class="col-sm">
                                                <div class="form-group">
                                                    Pollution Clearance Certificate
                                                    @Html.TextBoxFor(m => m.Pollution_Clearance_Certificate, new { @class = "form-control",type="file",required = "required",accept=".jpeg, .jpg, .png, .pdf",placeholder="" })
                                                    <img src="~/image/infoicon.png" title="Please Upload Pollution Clearance Certificate (Only .jpeg, .jpg, .png, .pdf file types are allowed and Max Size of Document is 700KB)" onclick="" />

