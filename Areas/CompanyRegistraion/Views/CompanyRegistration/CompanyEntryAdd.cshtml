<!-- Start State,City,Block -->
<script src="http://code.jquery.com/jquery-1.11.0.min.js"></script>
<script>
    var isCheckboxChecked = false;
    $('#State_Code').change(function () {
        debugger;
        if ($('#State_Code').val() != "") {
            $.ajax({

                url: '/UnitRegistration/UnitRegistration/GetDistrictByStateId',
                data: { 'State_Code': $('#State_Code').val() },
                dataType: 'json',
                type: 'post',
                success: function (data) {
                    debugger;
                    var row = "";
                    $('#District_Code').html('');
                    row += "<option value=''>-- Select District --</option>";
                    $.each(data, function (ind, item) {
                        row += "<option value='" + item.district_Code + "'>" + item.district_Name + "</option>";
                    });
                    $('#District_Code').html(row);
                },
                error: function (err) {

                }
            });
        }
        else {
            var District2 = "";
            $('#District_Code').html('');
            District2 += "<option value=''>-- Select District --</option>";
            $('#District_Code').html(District2);

            $('#Block_Code').empty();
            $('#Block_Code').append("<option value=''>-- Select Block --</option>");


        }
    });

    $('#District_Code').change(function () {
        debugger;
        if ($('#District_Code').val() != "") {
            $.ajax({

                url: '/UnitRegistration/UnitRegistration/GetBlockByDistrictId',
                data: {
                    'State_Code': $('#State_Code').val(),
                    'District_Code': $('#District_Code').val()
                },
                dataType: 'json',
                type: 'post',
                success: function (data) {
                    debugger;
                    var row = "";
                    $('#Block_Code').html('');
                    row += "<option value=''>-- Select Block --</option>";
                    $.each(data, function (ind, item) {
                        row += "<option value='" + item.block_Code + "'>" + item.block_Name + "</option>";
                    });
                    $('#Block_Code').html(row);
                },
                error: function (err) {

                }
            });
        }
        else {

            $('#Block_Code').empty();
            $('#Block_Code').append("<option value=''>-- Select Block --</option>");


        }
    });
    //  register ofiice
    $('#RState_Code').change(function () {

        if ($('#RState_Code').val() != "") {
            $.ajax({
                url: '/UnitRegistration/UnitRegistration/GetDistrictByStateId',
                data: { 'State_Code': $('#RState_Code').val() },
                dataType: 'json',
                type: 'post',
                success: function (data) {

                    var row = "";
                    $('#RDistrict_Code').html('');
                    row += "<option value=''>-- Select District --</option>";
                    $.each(data, function (ind, item) {
                        row += "<option value='" + item.district_Code + "'>" + item.district_Name + "</option>";
                    });
                    $('#RDistrict_Code').html(row);

                },
                error: function (err) {
                    console.log(err);
                }
            });
        } else {
            var District2 = "";
            $('#RDistrict_Code').html('');
            District2 += "<option value=''>-- Select District --</option>";
            $('#RDistrict_Code').html(District2);

            $('#RBlock_Code').empty();
            $('#RBlock_Code').append("<option value=''>-- Select Block --</option>");
        }

    });

    $('#RDistrict_Code').change(function () {
        if ($('#RDistrict_Code').val() != "") {
            $.ajax({
                url: '/UnitRegistration/UnitRegistration/GetBlockByDistrictId',
                data: {
                    'State_Code': $('#RState_Code').val(),
                    'District_Code': $('#RDistrict_Code').val()
                },
                dataType: 'json',
                type: 'post',
                success: function (data) {

                    var row = "";
                    $('#RBlock_Code').html('');
                    row += "<option value=''>-- Select Block --</option>";
                    $.each(data, function (ind, item) {
                        row += "<option value='" + item.block_Code + "'>" + item.block_Name + "</option>";
                    });
                    $('#RBlock_Code').html(row);

                },
                error: function (err) {
                    console.log(err);
                }
            });
        } else {
            $('#RBlock_Code').empty();
            $('#RBlock_Code').append("<option value=''>-- Select Block --</option>");
        }

    });
    // check ext.
    $(document).ready(function () {
        $('input[type="file"]').change(function () {
            var file = $(this)[0].files[0];
            var validExtensions = ['.jpeg', '.jpg', '.png', '.pdf'];

            var isValidExtension = validExtensions.some(function (extension) {
                return file.name.toLowerCase().endsWith(extension);
            });

            if (!isValidExtension) {
                alert('Invalid file extension! Please select a .jpeg, .jpg, .png, or .pdf file.');
                $(this).val('');
            }
        });
    });

    //check box
     $(document).ready(function () {

    $('#autoFillCheckbox').change(function () {

        if ($(this).is(':checked')) {
            isCheckboxChecked = true;
            $('#communicationAddress').val($('#registeredAddress').val());
            $('#State_Code').val($('#RState_Code').val());

            // Copy the selected option from RDistrict_Code to District_Code
            const select = document.getElementById("RDistrict_Code");
            const selectedIndex = select.selectedIndex;
            const selectedValue = select.value;
            const selectedText = select.options[selectedIndex].text;

            row = "<option value='" + selectedValue + "'>" + selectedText + "</option>";
            $('#District_Code').html(row);

            // Copy the selected option from RBlock_Code to Block_Code
            const select2 = document.getElementById("RBlock_Code");
            const selectedIndex2 = select2.selectedIndex;
            const selectedValue2 = select2.value;
            const selectedText2 = select2.options[selectedIndex2].text;

            row2 = "<option value='" + selectedValue2 + "'>" + selectedText2 + "</option>";
            $('#Block_Code').html(row2);

            // $('#Block_Code').val($('#RBlock_Code').val());
            $('#Pincode').val($('#RPincode').val());
        } else {
            $('#communicationAddress').val('');
            $('#State_Code').val('');
            $('#District_Code').val('');
            $('#Block_Code').val('');
            $('#Pincode').val('');
        }
    });
