                                                                    if (item.District_Code == Model.District_Code)
                                                                    {
                                                                                        <option selected="selected" value="@item.District_Code">@item.District_Name</option>
                                                                    }
                                                                    else
                                                                    {
                                                                                        <option value="@item.District_Code">@item.District_Name</option>
                                                                    }
                                                                }
                                                            }
                                                        }
                                                    </select>
@*                                                  <img src="~/image/infoicon.png" title="Please Select District" onclick="" />
*@
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">

                                            <div class="col-sm">
                                                <div class="form-group">
                                                    Block <span class="RequiredField"></span>
                                                    <select name="Block_Code" id="Block_Code" class="form-control searchableselect">
                                                        <option value="">-- Select Block --</option>
                                                        @{
                                                            List<CPDMSEF.MasterEF> lstBlockModel = (List<CPDMSEF.MasterEF>)ViewData["BlockDetails"];
                                                            if (lstBlockModel != null)
                                                            {
                                                                foreach (var item in lstBlockModel)
                                                                {
                                                                    if (item.Block_Code == Model.Block_Code)
                                                                    {
                                                                                        <option selected="selected" value="@item.Block_Code">@item.Block_Name</option>
                                                                    }
                                                                    else
                                                                    {
                                                                                        <option value="@item.Block_Code">@item.Block_Name</option>
                                                                    }
                                                                }
                                                            }
                                                        }
                                                    </select>
@*                                                    <img src="~/image/infoicon.png" title="Please Select Block " onclick="" />
*@
                                                </div>
                                            </div>

                                            <div class="col-sm">
                                                <div class="form-group">
                                                    Pin Code <span class="RequiredField">*</span>
                                                    @Html.TextBoxFor(m => m.Pincode, new { @class = "form-control" ,required = "required", type="number" ,pattern="[0-9]{4}", maxlength="6", oninput="javascript: if (this.value.length > this.maxLength) this.value = this.value.slice(0, this.maxLength);",placeholder="Enter PinCode" })
@*                                                    <img src="~/image/infoicon.png" title="Please Enter Pin Code " onclick="" />
